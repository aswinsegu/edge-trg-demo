<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OA-RefreshToken">
    <DisplayName>OA-RefreshToken</DisplayName>
    <Operation>RefreshAccessToken</Operation>
